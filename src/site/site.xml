<?xml version="1.0" encoding="ISO-8859-1"?>

<project name="OZ Messages"
         xmlns="http://maven.apache.org/DECORATION/1.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd">

    <bannerLeft>
        <name>OZ Messages library</name>
        <src>images/company-logo-min.png</src>
        <href>http://www.orbitalzero.com</href>
    </bannerLeft>

    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.2.1</version>
    </skin>

    <custom>
        <fluidoSkin>
            <sideBarEnabled>true</sideBarEnabled>
            <!--<googlePlusOne />-->
        </fluidoSkin>
    </custom>


    <body>
        <links>
            <item name="${company.name}" href="${company.website}"/>
        </links>

